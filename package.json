{
  "name": "yieldweaver-aggregator",
  "version": "1.0.0",
  "description": "YieldWeaver - Comprehensive DeFi Yield Farming Platform with Wallet Integration and Real-time APY Monitoring",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "nodemon server.js",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../contracts && npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "yield-farming",
    "defi",
    "blockchain",
    "ethers",
    "api",
    "react",
    "typescript",
    "smart-contracts",
    "wallet-integration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ethers": "^6.8.1",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
